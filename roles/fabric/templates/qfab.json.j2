{
  "Xpaths": {
    "install_path": "{{ eluvio_base_dir }}",
    "qparts_path": "{{ fabric_data }}/parts",
    "qtemp_path": "{{ fabric_data }}/temp",
    "qlibs_path": "{{ fabric_data }}/libs"
  },
  "fabric_store": {
    "local": {
      "enabled": true
    }
  },
  "fabric": {
    "Xnode_id": "inodcDKJHX6PDWxTE7nrVgCjn",
    "net": {
      "routing_protocol": {
        "type": "static_routes",
        "partition_strategy": {
          "partition_level": 1,
          "partition_num": 16,
          "partition_replication": 1
        }
      }
    }
  },
  "api": {
    "base_url": "http://:{{ api_port}}",
    "auth_bypass": {
      "username": "demo"
    },
    "cors": {
      "allowed_origins": [
        "*"
      ],
      "allowed_methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "OPTIONS"
      ],
      "allowed_headers": [
        "*"
      ],
      "max_age": 600,
      "debug": false
    },
    "log_body_max": "64KB"
  },
  "log": {
    "level": "debug",
    "formatter": "text",
    "file": {
      "filename": "{{ fabric_data }}/qfab.log",
      "maxsize": 10,
      "maxage": 0,
      "maxbackups": 2
    }
  },
  "llvm": {
    "execution_engine_cache": {
      "enabled": true,
      "size": 20
    },
    "port_range": "7000:7100"
  },
  "metrics": {
    "type": "expvar"
  },
  "qspaces": [
    {
      "id": "{{ space_id }}",
      "type": "Ethereum",
      "ethereum" : {"url":"http://127.0.0.1:{{ elvmasterd_rpc_port }}", "network_id":{{ blockchain_network }}}
    }
  ]
}
